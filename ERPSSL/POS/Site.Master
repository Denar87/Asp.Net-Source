            color: white;
            text-decoration: none;
        }

        .navbar1 {
            /*height:35px;
        min-height:40px;*/
            height: 30px;
            min-height: 30px;
        }

        .hm_sec_2_content {
            background: #fff none repeat scroll 0 0;
