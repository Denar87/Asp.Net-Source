    <link href="../css/jquery.mCustomScrollbar.css" rel="stylesheet" />
    <script>
        (function ($) {
            $(window).load(function () {

                $("#content-1").mCustomScrollbar({
                    autoHideScrollbar: true,
                    theme: "light"
                });

            });
        })(jQuery);
    </script>
    <script>
        (function ($) {
            $(window).load(function () {

                $("#content-2").mCustomScrollbar({
                    autoHideScrollbar: true,
                    theme: "light"
                });

            });
        })(jQuery);
